<div class="container white-text">

    <h2 class="green-text darken-1">Add New Player</h2>

    <%= form_for @player do |f| %>
    <div class="field">
        <%= f.label :name %>
        <%= f.text_field :name %>
    </div>
    <div class="field">
        <%= f.label :grade %>
        <%= f.text_field :grade %>
    </div>
    <div class="field">
        <%= f.label :number %>
        <%= f.text_field :number %>
    </div>
    <div class="field">
        <%= f.label :position1 %>
        <%= f.text_field :position1 %>
    </div>
    <div class="field">
        <%= f.label :position2 %>
        <%= f.text_field :position2 %>
    </div>
    <div class="field">
        <%= f.label :position3 %>
        <%= f.text_field :position3 %>
    </div>
    <div class="field">
        <%= f.label :coordinated %>
        <%= f.text_field :coordinated %>
    </div>
    <div class="field">
        <%= f.label :club_exp %>
        <%= f.text_field :club_exp %>
    </div>
    <%= f.file_field :image %>




    <div class="actions black-text"><%= f.submit %></div>
    <% end %>

    <%= link_to 'Back', players_path %>
</div>

